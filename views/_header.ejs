<header id = "heder">
    <h1>Header</h1>

    <% if(locals.user){%>

        <li>
            <a href="/users/profile/<%= user.id%>">
                <%= user.name %>
            </a>
        </li>
        <li>
            <a href="/users/sign-out">Sign Out</a>
        </li>
    <% } else{%>
          <li>
            <a href="/users/sign-in">Sign In</a>
          </li>

          <li>
            <a href="/users/sign-up">Sign Up</a>
          </li>
        <%}%>
   </header>